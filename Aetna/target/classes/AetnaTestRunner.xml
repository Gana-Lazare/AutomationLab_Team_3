<?xml version="1.0" encoding="UTF-8" ?>
        <!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="FoxNews Automation Test">
<!-- thread-count="2" parallel="tests"-->
<!-- Cross Browser Testing-->
<!-- Parameter to pass the value to WebAPI-->
<parameter name="useCloudEnv" value="false"/>
<parameter name="cloudEnvName" value="browserstack"/>
<!--<parameter name="cloudEnvName" value="saucelab"/>-->
<!-- <parameter name="os" value="windows"/>-->
<parameter name="os" value="OS X"/>
<!-- <parameter name="os_version" value="10"/>-->
<parameter name="os_version" value="catalina"/>
<parameter name="browserName" value="chrome"/>
<parameter name="browserVersion" value="85"/>
<parameter name="url" value="https://www.aetna.com/"/>

<test name="FoxNews Home">
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="85"/>
    <classes>
        <class name="aetnaHomePageTest.AetnaHomePageTest">
            <methods>
                <include name="searchBoxTest"></include>

                <exclude name=""></exclude>
            </methods>
        </class>
    </classes>
</test>
</suite>